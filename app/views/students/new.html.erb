User Profile
<hr>
<%= form_for @student do |f| -%>
    UserName: <%= f.text_field :username %><br />
    Password: <%= f.text_field :password %><br />
    First Name: <%= f.text_field :firstname %><br />
    Last Name: <%= f.text_field :lastname %><br />
    Date Of Birth: <%= f.text_field :dateofbirth %><br />
    Security Question: <%= f.text_field :securityquestion %><br />
    Answer: <%= f.text_field :answer %><br />
    Location: <%= f.text_field :location %><br />
  <%= f.submit %> <%= f.cancel %>
<% end -%>
